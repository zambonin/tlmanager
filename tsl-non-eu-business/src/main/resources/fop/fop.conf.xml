<?xml version="1.0"?>
<fop version="2.0">

  <default-page-settings height="29.7cm" width="21cm"/>
  
  <renderers>
    <renderer mime="application/pdf">

      <fonts>
          <font metrics-url="TIMES.xml" kerning="yes" embed-url="TIMES.TTF">
              <font-triplet name="Times New Roman" style="normal" weight="normal"/>
          </font>
          <font metrics-url="TIMESBD.xml" kerning="yes" embed-url="TIMESBD.TTF">
              <font-triplet name="Times New Roman" style="normal" weight="bold"/>
          </font>
          <font metrics-url="TIMESBI.xml" kerning="yes" embed-url="TIMESBI.TTF">
              <font-triplet name="Times New Roman" style="italic" weight="bold"/>
          </font>
          <font metrics-url="TIMESI.xml" kerning="yes" embed-url="TIMESI.TTF">
              <font-triplet name="Times New Roman" style="italic" weight="normal"/>
          </font>
      </fonts>

    </renderer>

  </renderers>

</fop>